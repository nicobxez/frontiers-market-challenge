{
  "extends": "./tsconfig.json",
  "include": [".eslintrc.cjs", "jest.config.js", ".ts", ".tsx"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "types": ["node"],
    "target": "ES2017",
    "module": "ESNext",
    "lib": ["ES2017", "DOM", "DOM.Iterable", "ESNext", "ES6", "ES5"],
    "noUnusedLocals": true
  }
}
